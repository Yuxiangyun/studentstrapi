{
  "kind": "collectionType",
  "collectionName": "students",
  "info": {
    "singularName": "student",
    "pluralName": "students",
    "displayName": "Student"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "studentNo": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "gender": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Male",
        "Female",
        "Other"
      ]
    },
    "className": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email"
    },
    "phone": {
      "type": "string"
    },
    "tags": {
      "type": "json",
      "required": false
    }
  }
}
